# Server Configuration
PORT=3001
NODE_ENV=development

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/restaurant_ar?schema=public"

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production
